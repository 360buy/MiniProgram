<block>
    <view class="container">
        <view class="page-body">
            <view class="page-section">
                <view class="page-section-title"><text>导向要素</text>
                </view>
                <view>
                    <picker mode="selector" range="{{honor}}" bindchange="onChange">
                        <view class="picker">荣誉称号：<text class="itemText">{{honorChecked}}</text>
                        </view>
                    </picker>
                </view>
                <view>
                    <picker mode="selector" range="{{learning}}" bindchange="onChange">
                        <view class="picker">学术、文体竞赛获奖：<text class="itemText">{{learningChecked}}</text>
                        </view>
                    </picker>
                </view>
            </view>
        </view>
    </view>
</block>